{
  "name": "TimeLimit Server",
  "version": "1.0.2",
  "slug": "timelimit",
  "description": "Self-hosted TimeLimit server (SQLite)",
  "startup": "services",
  "boot": "auto",
  "ports": {
    "8080/tcp": 8080
  },
  "webui": "http://[HOST]:[PORT:8080]",
  "ingress": false,
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "map": [
    "share:rw",
    "data:rw"
  ],
  "options": {
    "admin_token": "setup:initial-admin"
  },
  "schema": {
    "admin_token": "str"
  },
  "environment": {
    "ADMIN_TOKEN": "{admin_token}",
    "ADMIN_PASSWORD": "changeme"
  },
  "image": "ghcr.io/jeroenlind/timelimit-ha:{version}"
}
